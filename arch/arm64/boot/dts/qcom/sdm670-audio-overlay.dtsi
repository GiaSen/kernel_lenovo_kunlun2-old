/* huaqin add for JD2019-123 by xudayi at 2018/08/31 start */
&tlmm {
	cs35l41_reset_default: cs35l41_reset_default {
		config {
#if defined(CONFIG_PRODUCT_ZAP)
			pins = "gpio54";
#else
			pins = "gpio93";
#endif
			output-low;
			bias-pull-up;
			drive-strength = <2>;
		};
	};

	/* huaqin add for JD2019-303 by xudayi at 2018/09/12 start */
	cs35l41_irq_default: cs35l41_irq_default {
		config {
#if defined(CONFIG_PRODUCT_ZAP)
			pins = "gpio123";
#else
			pins = "gpio26";
#endif
			drive-strength = <2>;
			bias-pull-up;
		};
	};
	/* huaqin add for JD2019-303 by xudayi at 2018/09/12 end */

	cs35l41_r_reset_default: cs35l41_r_reset_default {
		config {
#if defined(CONFIG_PRODUCT_ZAP)
			pins = "gpio4";
#else
			pins = "gpio50";
#endif
			output-low;
			bias-pull-up;
			drive-strength = <2>;
		};
	};
	/* huaqin add for JD2019-303 by xudayi at 2018/09/12 start */
	cs35l41_r_irq_default: cs35l41_r_irq_default {
		config {
#if defined(CONFIG_PRODUCT_ZAP)
			pins = "gpio26";
#else
			pins = "gpio49";
#endif
			drive-strength = <2>;
			bias-pull-up;
		};
	};
	/* huaqin add for JD2019-303 by xudayi at 2018/09/12 end */
#if defined(CONFIG_PRODUCT_ZAP)
	igo1201_reset_default: igo1201_reset_default {
		config {
			pins = "gpio79";
			output-low;
			bias-pull-up;
			drive-strength = <2>;
		};
	};

	aud_soc_mode_sel_default: aud_soc_mode_sel_default {
		config {
			pins = "gpio5";
			output-low;
			bias-pull-up;
			drive-strength = <2>;
