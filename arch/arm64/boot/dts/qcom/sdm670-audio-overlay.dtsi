#if defined(CONFIG_PRODUCT_ZAP)
&qupv3_se7_i2c {
        status = "okay";

	/* Speaker Amp */
	cs35l41:cs35l41@40 {
		status = "okay";
		compatible = "cirrus,cs35l41";
		reg = <0x40>;

		reset-gpios = <&tlmm 54 0>;
		interrupt-parent = <&tlmm>;
		interrupts = <123 0>;
		interrupt-names = "amp_l_irq";

		cirrus,right-channel-amp; /* Set Speaker Amp as default Right channel */
		cirrus,boost-peak-milliamp = <4500>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,asp-sdout-hiz = <0x3>;

		pinctrl-names = "reset";
		pinctrl-0 = <&cs35l41_reset_default>;
		cirrus,gpio-config2 {
			cirrus,gpio-type = <1>; /* 0 indicate gpio, 1 indicate interrupt */
			cirrus,gpio-src-select = <0x4>;
			cirrus,gpio-output-enable;
		};
 	};

	/* Receiver Amp */
	cs35l41_r:cs35l41_r@41 {
		status = "okay";
		compatible = "cirrus,cs35l41";
		reg = <0x41>;

		reset-gpios = <&tlmm 4 0>;
		interrupt-parent = <&tlmm>;
		interrupts = <26 0>;
		interrupt-names = "amp_r_irq";

		cirrus,boost-peak-milliamp = <4500>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,asp-sdout-hiz = <0x3>;
		cirrus,dsp-recovery;
		pinctrl-names = "reset";
		pinctrl-0 = <&cs35l41_r_reset_default>;
		cirrus,gpio-config2 {
			cirrus,gpio-type = <1>; /* 0 indicate gpio, 1 indicate interrupt */
			cirrus,gpio-src-select = <0x4>;
			cirrus,gpio-output-enable;
		};
	};

	debussy:debussy@5d {
			status = "okay";
			compatible = "intelligo,debussy";
			reset-gpios = <&tlmm 79 0>;
			deb-gpios = <&tlmm 40 0>;
			aud_soc_mode_sel-gpios = <&tlmm 5 0>;
			power_en_gpio-gpios = <&tlmm 27 0>;
			reg = <0x5d>;
			interrupt-parent = <&tlmm>;
			interrupts = <40 IRQ_TYPE_EDGE_RISING>;
			debounce = <5>;
			clock-names = "igo_mclk";
			clocks = <&clock_rpmh RPMH_LN_BB_CLK2>;

			pinctrl-names = "reset", "aud_soc_mode_sel", "power_en_gpio";
			pinctrl-0 = <&igo1201_reset_default>;
			pinctrl-1 = <&aud_soc_mode_sel_default>;
			pinctrl-2 = <&power_en_gpio_default>;
	};
};
