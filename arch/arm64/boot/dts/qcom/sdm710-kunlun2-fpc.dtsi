
&soc {
    fpc1020 {
        status = "ok";
        compatible = "fpc,fpc1020";
        interrupt-parent = <&tlmm>;
        interrupts = <121 0x0>;
        fpc,gpio_rst    = <&tlmm 134 0x0>;
        fpc,gpio_irq    = <&tlmm 121 0x0>;
        fpc,gpio_vdd    = <&tlmm 90 0x0>;
        /*vcc_spi-supply  = <&pm8994_s4>;*/
        /*vdd_io-supply   = <&pm8996_l18>;*/
        /*vdd_ana-supply  = <&pm8996_l18>;*/
        /* fpc,enable-on-boot; */
        /* fpc,enable-wakeup; */

        pinctrl-names = "fpc1020_reset_reset",
            "fpc1020_reset_active",
            "fpc1020_irq_active";

        pinctrl-0 = <&msm_gpio_134>;
        pinctrl-1 = <&msm_gpio_134_output_high>;
        pinctrl-2 = <&msm_gpio_121>;
    };

	qcom,qbt1000 {
		compatible = "qcom,qbt1000";
		clock-names = "core", "iface";
		clock-frequency = <25000000>;
		qcom,finger-detect-gpio = <&tlmm 122 0>;
	};

};

