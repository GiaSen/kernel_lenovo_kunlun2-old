&tlmm {
    /* GPIO_21 : FP_RESET_N */
msm_gpio_134: msm_gpio_134 {
                  mux {
                      pins = "gpio134";
                      function = "gpio";
                  };

                  config {
                      pins = "gpio134";
                      drive-strength = <2>;
                      bias-disable;
                      output-low;
                  };
              };

              /* GPIO_21 : FP_RESET_N, state device active*/
msm_gpio_134_output_high: msm_gpio_134_output_high {
                              mux {
                                  pins = "gpio134";
                                  function = "gpio";
                              };

                              config {
                                  pins = "gpio134";
                                  drive-strength = <2>;
                                  bias-disable;
                                  output-high;
                              };
                          };

                          /* GPIO_134 : FP_INT_N */
msm_gpio_121: msm_gpio_121 {
                  mux {
                      pins = "gpio121";
                      function = "gpio";
                  };

                  config {
                      pins = "gpio121";
                      drive-strength = <2>;
                      bias-pull-down;
                  };
              };
};

